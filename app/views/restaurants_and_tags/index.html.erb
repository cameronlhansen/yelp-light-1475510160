<div class="page-header">
  <h1>
    Restaurants and tags
    <a href="/restaurants_and_tags/new" class="btn btn-lg btn-success">New Restaurants and tag</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Restaurant</th>
        <th>Tag</th>
        <th>Actions</th>
      </tr>

      <% @restaurants_and_tags.each do |restaurants_and_tag| %>
      <tr>
        <td><%= restaurants_and_tag.restaurant_id %></td>
        <td><%= restaurants_and_tag.tag_id %></td>
        <td>
          <a href="/restaurants_and_tags/<%= restaurants_and_tag.id %>" class="btn btn-primary">Show</a>
          <a href="/restaurants_and_tags/<%= restaurants_and_tag.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_restaurants_and_tag/<%= restaurants_and_tag.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
