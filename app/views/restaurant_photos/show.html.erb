<div class="page-header">
  <h3>Restaurant Photo #<%= @restaurant_photo.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @restaurant_photo.user_id %></dd>

      <dt>Restaurant</dt>
      <dd><%= @restaurant_photo.restaurant_id %></dd>

      <dt>Review</dt>
      <dd><%= @restaurant_photo.review_id %></dd>

      <dt>Photo</dt>
      <dd><%= @restaurant_photo.photo %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/restaurant_photos" class="btn btn-primary">
        Back
      </a>
      <a href="/restaurant_photos/<%= @restaurant_photo.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_restaurant_photo/<%= @restaurant_photo.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
